<div class="event-detail">
	<div class='event-detail-block event-detail-location'>
	  {% include 'atoms-h4' with { 'content': 'Where', 'class': 'b4 event-detail-location_title' } %}
	  {% include 'atoms-paragraph' with { 'content': 'Washington Court Hotel<br/>525 New Jersey Ave, NW<br/>Washington, DC 20001V', 'class': 'event-detail-description event-detail-location_description' } %}
	  {% include 'atoms-link' with { 'content': 'View on Google Maps', 'class': 'link-blue event-detail-location_link' } %}
	</div>
	<div class='event-detail-block event-detail-time'>
	  {% include 'atoms-h4' with { 'content': 'When', 'class': 'b4 event-detail-time_title' } %}
	  {% include 'atoms-paragraph' with { 'content': 'March 10–12, 2016<br/>9 A.M.–5 P.M.', 'class': 'event-detail-description event-detail-time_description' } %}
	</div>
	<div class='event-detail-block event-detail-register'>
	  {% include 'atoms-h4' with { 'content': 'Register', 'class': 'b4 event-detail-register_title' } %}
	  {% include 'atoms-paragraph' with { 'content': 'Proin quis ante quam. Duis sit amet risus sit amet elit dapibus dictum. Integer imperdiet dolor tortor a auctor quamla. Bibendum non donec tincidunt ultrices dapibus. Suspendisse vitae enim efficitur.', 'class': 'event-detail-description event-detail-register_title' } %}
	  {% include 'atoms-button-link' with { 'content': 'Register Now', 'class': 'event-detail-register_cta' } %}
	</div>
</div>
